  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon --watch server --ext ts,tsx --exec tsx server/index.ts",
    "client:dev": "cd client && vite",
    "build": "cd client && vite build && cd .. && esbuild server/index.ts --bundle --platform=node --format=esm --outdir=dist --alias:@shared=./shared --packages=external --external:compression --external:sirv && cp -R server/migrations dist/migrations",
    "start": "NODE_ENV=production node --import tsx -r tsconfig-paths/register dist/index.js",
    "seed": "tsx server/seed.ts",
    
    "db:push": "drizzle-kit push:pg --accept-data-loss"
  },
